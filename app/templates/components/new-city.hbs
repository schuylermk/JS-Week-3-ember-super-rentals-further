{{if addNewCity}}
<h1>New City this is app/temp/comp/new-city</h1>
<div class="container">
  <form>
    <div class="form-group">
      <label for="name">Name</label>
      {{input value=name id="name"}}
    </div>
    <div class="form-group">
      <label for="country">Country</label>
      {{input value=country id="country"}}
    </div>
    <button {{action 'save4'}}>Save</button>
  </form>
</div>

{{else}}
<button {{action 'cityFormShow'}}> New City</button>
{{/if}}
